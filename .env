# ==================================================
# Global settings
# ==================================================

# Timezone used by containers and applications
TZ=Asia/Shanghai

# ==================================================
# Database connection settings (DISABLED by default)
#
# 说明：
# - 以下数据库连接信息默认全部注释
# - 只有在需要直连数据库时才取消注释
# - 推荐通过 docker-compose.db.yml 或 CI/CD 注入
# ==================================================


# --------------------------------------------------
# GAUSSDB connection settings
# --------------------------------------------------
# GAUSSDB_HOST=localhost        # 数据库主机名
# GAUSSDB_PORT=5432             # 数据库端口
# GAUSSDB_DBNAME=gaussdb        # 数据库名
# GAUSSDB_USER=gaussdb          # 用户名
# GAUSSDB_PASSWORD=P@ssw1rd     # 密码


# --------------------------------------------------
# VERTICA connection settings
# --------------------------------------------------
# VERTICA_HOST=localhost        # 数据库主机名
# VERTICA_PORT=5433             # 数据库端口
# VERTICA_DBNAME=vertica        # 数据库名
# VERTICA_USER=vertica          # 用户名
# VERTICA_PASSWORD=P@ssw1rd     # 密码


# --------------------------------------------------
# ODPS connection settings
# --------------------------------------------------
# ODPS_ACCESS_ID="<your-access-id>"     # 阿里云 AccessKey ID
# ODPS_ACCESS_KEY="<your-access-key>"   # 阿里云 AccessKey Secret
# ODPS_PROJECT="<your-project>"         # ODPS 项目名
